import random
print("Welcome to the Dice Rolling Simulator ðŸŽ²")

while True:
    User = input("Press Enter to roll the dice: (or 'q' to quit) ")
    if User.lower() == "q":
        print("Game Over ðŸ‘‹")
        break

    dice = random.choice(["1", "2", "3", "4", "5", "6"])
    print("Result:", dice)
